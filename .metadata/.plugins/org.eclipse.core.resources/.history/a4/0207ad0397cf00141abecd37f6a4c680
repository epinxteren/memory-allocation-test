# All Target
all: Mem.axf

# Tool invocations
Mem.axf: $(OBJS) $(USER_OBJS)
	@echo 'Building target: $@'
	@echo 'Invoking: MCU Linker'
	arm-none-eabi-gcc -nostdlib -L"/Users/eric/sites/test-lpc/CMSIS_CORE_LPC17xx/Debug" -L"/Users/eric/sites/test-lpc/CMSIS_DSPLIB_CM3/lib" -Xlinker -Map="Mem.map" -Xlinker --gc-sections -mcpu=cortex-m3 -mthumb -T "Mem_Debug.ld" -o "Mem.axf" $(OBJS) $(USER_OBJS) $(LIBS)
	@echo 'Finished building target: $@'
	@echo ' '
	$(MAKE) --no-print-directory post-build

# Other Targets
clean:
	-$(RM) $(OBJS)$(C_DEPS)$(EXECUTABLES) Mem.axf
	-@echo ' '

post-build:
	-@echo 'Performing post-build steps'
	-arm-none-eabi-size "Mem.axf"; # arm-none-eabi-objcopy -v -O binary "Mem.axf" "Mem.bin" ; # checksum -p LPC1768 -d "Mem.bin";
	-@echo ' '

